<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Shown when an error occurs while loading the app -->
    <string name="offline_heading">Esteu desconnectats.</string>

    <!-- Button to retry loading the app after an error -->
    <string name="offline_description">Premeu aquí per tornar-ho a intentar!</string>

    <!-- Shown when the app tries to use a feature that needs an external app, but the app is not found -->
    <string name="noapp_heading">No s\'ha trobat l\'aplicació.</string>

    <!-- Shown when the app tries to use a feature that needs an external app, but the app is not found -->
    <string name="noapp_description">Cap aplicació en aquest dispositiu pot realitzar aquesta acció.</string>

    <!-- Shown when the user tries to upload a photo for a place -->
    <string name="camera_access_required">Per afegir una foto, permeteu l\'accés a la càmera.</string>

    <!-- Button caption used in multiple places -->
    <string name="ok">D\'acord</string>

    <!-- Shown when the device has no built-in camera, but the user tries to add a place photo -->
    <string name="device_has_no_camera">Ho sentim, només es poden afegir fotos des de telèfons amb càmera. </string>

    <!-- Shown when a storage problem occurs while taking a place photo -->
    <string name="cannot_generate_image_tempfile">Ho sentim, no s\'ha pogut guardar l\'imatge. Teniu prou espai lliure?</string>

    <!-- Shown when the user cancels the camera while adding a place photo -->
    <string name="no_image_uploaded">No s\'ha carregat cap imatge.</string>
</resources>
