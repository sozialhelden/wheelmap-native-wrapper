<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Shown when an error occurs while loading the app -->
    <string name="offline_heading">Είστε εκτός σύνδεσης.</string>

    <!-- Button to retry loading the app after an error -->
    <string name="offline_description">Πατήστε εδώ για να προσπαθήσετε ξανά!</string>

    <!-- Shown when the app tries to use a feature that needs an external app, but the app is not found -->
    <string name="noapp_heading">Η εφαρμογή δεν βρέθηκε.</string>

    <!-- Shown when the app tries to use a feature that needs an external app, but the app is not found -->
    <string name="noapp_description">Καμία εφαρμογή σε αυτήν τη συσκευή δεν μπορεί να εκτελέσει αυτήν την ενέργεια.</string>

    <!-- Shown when the user tries to upload a photo for a place -->
    <string name="camera_access_required">Για να προσθέσετε μια φωτογραφία, παρακαλούμε επιτρέψτε την πρόσβαση στην κάμερα.</string>

    <!-- Button caption used in multiple places -->
    <string name="ok">Εντάξει</string>

    <!-- Shown when the device has no built-in camera, but the user tries to add a place photo -->
    <string name="device_has_no_camera">Λυπούμαστε, η προσθήκη φωτογραφιών λειτουργεί μόνο σε συσκευές με κάμερα.</string>

    <!-- Shown when a storage problem occurs while taking a place photo -->
    <string name="cannot_generate_image_tempfile">Λυπούμαστε, δεν ήταν δυνατή η αποθήκευση της εικόνας. Έχετε αρκετό χώρο αποθήκευσης;</string>

    <!-- Shown when the user cancels the camera while adding a place photo -->
    <string name="no_image_uploaded">Δεν έγινε μεταφόρτωση εικόνας.</string>
</resources>
