<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Shown when an error occurs while loading the app -->
    <string name="offline_heading">आप ऑफ़लाइन हैं।</string>

    <!-- Button to retry loading the app after an error -->
    <string name="offline_description">दोबारा प्रयास करने के लिए यहाँ टैप करें!</string>

    <!-- Shown when the app tries to use a feature that needs an external app, but the app is not found -->
    <string name="noapp_heading">ऐप नहीं मिला।</string>

    <!-- Shown when the app tries to use a feature that needs an external app, but the app is not found -->
    <string name="noapp_description">इस डिवाइस का कोई भी ऐप यह क्रिया नहीं कर सकता।</string>

    <!-- Shown when the user tries to upload a photo for a place -->
    <string name="camera_access_required">फ़ोटो शामिल करने के लिए कैमरा एक्सेस करने की अनुमति दें।</string>

    <!-- Button caption used in multiple places -->
    <string name="ok">ठीक है</string>

    <!-- Shown when the device has no built-in camera, but the user tries to add a place photo -->
    <string name="device_has_no_camera">क्षमा करें, फ़ोटो शामिल करना केवल कैमरा युक्त डिवाइस पर ही काम करता है।</string>

    <!-- Shown when a storage problem occurs while taking a place photo -->
    <string name="cannot_generate_image_tempfile">क्षमा करें, इमेज को सहेज नहीं सकते। क्या आपके पास पर्याप्त स्टोरेज है?</string>

    <!-- Shown when the user cancels the camera while adding a place photo -->
    <string name="no_image_uploaded">कोई इमेज अपलोड नहीं हुआ।</string>
</resources>
