<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Shown when an error occurs while loading the app -->
    <string name="offline_heading">Çevrimdışısınız.</string>

    <!-- Button to retry loading the app after an error -->
    <string name="offline_description">Tekrar denemek için dokunun!</string>

    <!-- Shown when the app tries to use a feature that needs an external app, but the app is not found -->
    <string name="noapp_heading">Uygulama bulunamadı.</string>

    <!-- Shown when the app tries to use a feature that needs an external app, but the app is not found -->
    <string name="noapp_description">Bu cihazdaki hiçbir uygulama bu işlemi gerçekleştiremez.</string>

    <!-- Shown when the user tries to upload a photo for a place -->
    <string name="camera_access_required">Fotoğraf eklemek için lütfen kamera erişimine izin verin.</string>

    <!-- Button caption used in multiple places -->
    <string name="ok">Tamam</string>

    <!-- Shown when the device has no built-in camera, but the user tries to add a place photo -->
    <string name="device_has_no_camera">Üzgünüz, yalnızca kamerası olan cihazlarda fotoğraf ekleyebilirsiniz.</string>

    <!-- Shown when a storage problem occurs while taking a place photo -->
    <string name="cannot_generate_image_tempfile">Maalesef görüntü kaydedilemedi. Yeterli depolama alanınız var mı?</string>

    <!-- Shown when the user cancels the camera while adding a place photo -->
    <string name="no_image_uploaded">Görüntü yüklenemedi.</string>
