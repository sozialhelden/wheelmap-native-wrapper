<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Shown when an error occurs while loading the app -->
    <string name="offline_heading">Ste offline.</string>

    <!-- Button to retry loading the app after an error -->
    <string name="offline_description">Klepnutím sem to skúste znova!</string>

    <!-- Shown when the app tries to use a feature that needs an external app, but the app is not found -->
    <string name="noapp_heading">Aplikácia nebola nájdená.</string>

    <!-- Shown when the app tries to use a feature that needs an external app, but the app is not found -->
    <string name="noapp_description">Túto akciu nemôže vykonať žiadna aplikácia na tomto zariadení.</string>

    <!-- Shown when the user tries to upload a photo for a place -->
    <string name="camera_access_required">Ak chcete pridať fotku, povoľte prístup fotoaparátu.</string>

    <!-- Button caption used in multiple places -->
    <string name="ok">Dobre</string>

    <!-- Shown when the device has no built-in camera, but the user tries to add a place photo -->
    <string name="device_has_no_camera">Ľutujeme, pridávanie fotografií funguje iba na zariadeniach s fotoaparátom.</string>

    <!-- Shown when a storage problem occurs while taking a place photo -->
    <string name="cannot_generate_image_tempfile">Ľutujeme, obrázok sa nepodarilo uložiť. Máte dostatok úložného priestoru?</string>

    <!-- Shown when the user cancels the camera while adding a place photo -->
    <string name="no_image_uploaded">Nebol nahratý žiaden obrázok.</string>
</resources>
