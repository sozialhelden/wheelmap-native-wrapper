<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Shown when an error occurs while loading the app -->
    <string name="offline_heading">Et ole yhteydessä verkkoon.</string>

    <!-- Button to retry loading the app after an error -->
    <string name="offline_description">Napauta tästä yrittääksesi uudelleen!</string>

    <!-- Shown when the app tries to use a feature that needs an external app, but the app is not found -->
    <string name="noapp_heading">Sovellusta ei löytynyt.</string>

    <!-- Shown when the app tries to use a feature that needs an external app, but the app is not found -->
    <string name="noapp_description">Mikään sovellus tällä laitteella ei voi suorittaa tätä toimintoa.</string>

    <!-- Shown when the user tries to upload a photo for a place -->
    <string name="camera_access_required">Salli kameran käyttöoikeudet, jotta voit lisätä kuvan.</string>

    <!-- Button caption used in multiple places -->
    <string name="ok">Okei</string>

    <!-- Shown when the device has no built-in camera, but the user tries to add a place photo -->
    <string name="device_has_no_camera">Pahoittelut, mutta kuvien lisääminen toimii vain laitteilla, joissa on kamera.</string>

    <!-- Shown when a storage problem occurs while taking a place photo -->
    <string name="cannot_generate_image_tempfile">Pahoittelut, mutta kuvaa ei voitu tallentaa. Onko sinulla riittävästi tallennustilaa?</string>

    <!-- Shown when the user cancels the camera while adding a place photo -->
    <string name="no_image_uploaded">Kuvaa ei lisätty.</string>
</resources>
