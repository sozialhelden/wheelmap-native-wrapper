    <string name="noapp_description">אין אפליקציה במכשיר זה המסוגלת לבצע פעולה זו.</string>

    <!-- Shown when the user tries to upload a photo for a place -->
    <string name="camera_access_required">כדי להוסיף תמונה, נא לאפשר גישה למצלמה.</string>

    <!-- Button caption used in multiple places -->
    <string name="ok">אוקי</string>

