<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Shown when an error occurs while loading the app -->
    <string name="offline_heading">Você está offline.</string>

    <!-- Button to retry loading the app after an error -->
    <string name="offline_description">Toque aqui para tentar de novo!</string>

    <!-- Shown when the app tries to use a feature that needs an external app, but the app is not found -->
    <string name="noapp_heading">Aplicativo não encontrado.</string>

    <!-- Shown when the app tries to use a feature that needs an external app, but the app is not found -->
    <string name="noapp_description">Nenhum aplicativo neste dispositivo pode realizar esta ação.</string>

    <!-- Shown when the user tries to upload a photo for a place -->
    <string name="camera_access_required">Para adicionar uma foto, permita acesso à câmera.</string>

    <!-- Button caption used in multiple places -->
    <string name="ok">Está bem</string>

    <!-- Shown when the device has no built-in camera, but the user tries to add a place photo -->
    <string name="device_has_no_camera">Desculpe, adicionar fotos só é possível em dispositivos com câmera.</string>

    <!-- Shown when a storage problem occurs while taking a place photo -->
    <string name="cannot_generate_image_tempfile">Desculpe, não foi possível salvar a imagem. Você tem armazenamento suficiente?</string>

    <!-- Shown when the user cancels the camera while adding a place photo -->
    <string name="no_image_uploaded">Nenhuma imagem foi enviada.</string>
</resources>
