<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Shown when an error occurs while loading the app -->
    <string name="offline_heading">Bạn đang ngoại tuyến.</string>

    <!-- Button to retry loading the app after an error -->
    <string name="offline_description">Hãy nhấn vào đây để thử lại!</string>

    <!-- Shown when the app tries to use a feature that needs an external app, but the app is not found -->
    <string name="noapp_heading">Không tìm thấy ứng dụng.</string>

    <!-- Shown when the app tries to use a feature that needs an external app, but the app is not found -->
    <string name="noapp_description">Không ứng dụng nào trên thiết bị này có thể thực hiện hành động này.</string>

    <!-- Shown when the user tries to upload a photo for a place -->
    <string name="camera_access_required">Để thêm ảnh, vui lòng cho phép ứng dụng truy cập vào máy ảnh.</string>

    <!-- Button caption used in multiple places -->
    <string name="ok">OK</string>

    <!-- Shown when the device has no built-in camera, but the user tries to add a place photo -->
    <string name="device_has_no_camera">Rất tiếc, tính năng thêm ảnh chỉ hoạt động trên thiết bị có máy ảnh.</string>

    <!-- Shown when a storage problem occurs while taking a place photo -->
    <string name="cannot_generate_image_tempfile">Rất tiếc, không thể lưu được hình ảnh. Bạn có đủ dung lượng bộ nhớ không?</string>

    <!-- Shown when the user cancels the camera while adding a place photo -->
    <string name="no_image_uploaded">Không có hình ảnh nào được tải lên.</string>
</resources>
