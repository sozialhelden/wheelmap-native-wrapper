<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Shown when an error occurs while loading the app -->
    <string name="offline_heading">Está offline.</string>

    <!-- Button to retry loading the app after an error -->
    <string name="offline_description">Toque aqui para tentar novamente!</string>

    <!-- Shown when the app tries to use a feature that needs an external app, but the app is not found -->
    <string name="noapp_heading">Aplicação não encontrada.</string>

    <!-- Shown when the app tries to use a feature that needs an external app, but the app is not found -->
    <string name="noapp_description">Não existe uma aplicação neste dispositivo que consiga realizar esta ação.</string>

    <!-- Shown when the user tries to upload a photo for a place -->
    <string name="camera_access_required">Para adicionar uma foto, permita o acesso à câmara.</string>

    <!-- Button caption used in multiple places -->
    <string name="ok">OK</string>

    <!-- Shown when the device has no built-in camera, but the user tries to add a place photo -->
    <string name="device_has_no_camera">Só é possível adicionar fotos em dispositivos com uma câmara.</string>

    <!-- Shown when a storage problem occurs while taking a place photo -->
    <string name="cannot_generate_image_tempfile">Não foi possível guardar a imagem. Tem armazenamento suficiente?</string>

    <!-- Shown when the user cancels the camera while adding a place photo -->
    <string name="no_image_uploaded">Nenhuma imagem foi carregada.</string>
</resources>
