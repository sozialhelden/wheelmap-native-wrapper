<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Shown when an error occurs while loading the app -->
    <string name="offline_heading">Ви не в мережі.</string>

    <!-- Button to retry loading the app after an error -->
    <string name="offline_description">Торкніться тут, щоб повторити!</string>

    <!-- Shown when the app tries to use a feature that needs an external app, but the app is not found -->
    <string name="noapp_heading">Додаток не знайдено.</string>

    <!-- Shown when the app tries to use a feature that needs an external app, but the app is not found -->
    <string name="noapp_description">Жоден додаток на цьому пристрої не може виконати цю дію.</string>

    <!-- Shown when the user tries to upload a photo for a place -->
    <string name="camera_access_required">Щоб додати світлину, будь ласка, дозвольте доступ до камери.</string>

    <!-- Button caption used in multiple places -->
    <string name="ok">Добре</string>

    <!-- Shown when the device has no built-in camera, but the user tries to add a place photo -->
    <string name="device_has_no_camera">Вибачте, додавання світлин працює лише на пристроях із камерою.</string>

    <!-- Shown when a storage problem occurs while taking a place photo -->
    <string name="cannot_generate_image_tempfile">Вибачте, не вдалося зберегти зображення. Чи достатньо у вас пам\'яті?</string>

    <!-- Shown when the user cancels the camera while adding a place photo -->
    <string name="no_image_uploaded">Зображення не було завантажене.</string>
</resources>
