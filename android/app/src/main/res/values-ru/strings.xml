<?xml version="1.0" encoding="utf-8"?>
<resources>
    <!-- Shown when an error occurs while loading the app -->
    <string name="offline_heading">Вы не в сети.</string>

    <!-- Button to retry loading the app after an error -->
    <string name="offline_description">Нажмите здесь, чтобы попытаться снова!</string>

    <!-- Shown when the app tries to use a feature that needs an external app, but the app is not found -->
    <string name="noapp_heading">Приложение не найдено.</string>

    <!-- Shown when the app tries to use a feature that needs an external app, but the app is not found -->
    <string name="noapp_description">На устройстве нет приложений, способных выполнить данное действие.</string>

    <!-- Shown when the user tries to upload a photo for a place -->
    <string name="camera_access_required">Для добавления фото дайте, пожалуйста, доступ к камере.</string>

    <!-- Button caption used in multiple places -->
    <string name="ok">Хорошо</string>

    <!-- Shown when the device has no built-in camera, but the user tries to add a place photo -->
    <string name="device_has_no_camera">Извините, добавление фотографий работает только на устройствах с камерой.</string>

    <!-- Shown when a storage problem occurs while taking a place photo -->
    <string name="cannot_generate_image_tempfile">Извините, изображение сохранить не удалось. Достаточно ли на устройстве памяти?</string>

    <!-- Shown when the user cancels the camera while adding a place photo -->
    <string name="no_image_uploaded">Изображение не загружено.</string>
</resources>
